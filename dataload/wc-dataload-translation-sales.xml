<_config:DataLoadConfiguration
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.ibm.com/xmlns/prod/commerce/foundation/config ../xsd/wc-dataload.xsd"
        xmlns:_config="http://www.ibm.com/xmlns/prod/commerce/foundation/config">

        <_config:DataLoadEnvironment configFile="${envPath}wc-dataload-env.xml" />

        <_config:LoadOrder commitCount="1" batchSize="1" maxError="500" dataLoadMode="Replace">
        <_config:property name="firstTwoLinesAreHeader" value="true" />

        <_config:LoadItem name="CatalogGroupName" businessObjectConfigFile="wc-loader-catalog-group-name.xml">
          <_config:DataSourceLocation location="${inputLocation}catgrpdesc.csv" />
        </_config:LoadItem>
        <!--
        <_config:LoadItem name="CatalogEntryCatGrpRel" businessObjectConfigFile="wc-loader-catalog-entry-catgrp-rel.xml">
          <_config:DataSourceLocation location="../catentrycatgrprel-output.csv" />
        </_config:LoadItem>
        <_config:LoadItem name="CatalogEntryAssoc" businessObjectConfigFile="wc-loader-catalog-entry-assoc.xml">
          <_config:DataSourceLocation location="../catentryassoc-output.csv" />
        </_config:LoadItem>
        -->

    </_config:LoadOrder>

</_config:DataLoadConfiguration>
