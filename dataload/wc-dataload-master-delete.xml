<_config:DataLoadConfiguration
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.ibm.com/xmlns/prod/commerce/foundation/config ../xsd/wc-dataload.xsd"
        xmlns:_config="http://www.ibm.com/xmlns/prod/commerce/foundation/config">

        <_config:DataLoadEnvironment configFile="${envPath}wc-dataload-env.xml" />

        <_config:LoadOrder commitCount="1000" batchSize="500" maxError="500" dataLoadMode="Replace">
          <_config:property name="firstTwoLinesAreHeader" value="true" />
          <_config:property name="loadSEO" value="true"/>
          
          <_config:LoadItem name="CatalogEntryAttrRelDeletion" businessObjectConfigFile="wc-loader-catalog-entry-attr-rel.xml">
            <_config:DataSourceLocation location="${outputLocation}catentryattrrel-delete-output${batchfile}.csv" />
          </_config:LoadItem> 
          <_config:LoadItem name="CatalogEntryCatEntRelDel" businessObjectConfigFile="wc-loader-catalog-entry-catent-rel-del.xml">
            <_config:DataSourceLocation location="${outputLocation}catentrycatentreldel-output${batchfile}.csv" />
          </_config:LoadItem>
          <_config:LoadItem name="CatalogEntryCatEntRelNew" businessObjectConfigFile="wc-loader-catalog-entry-catent-rel-new.xml">
            <_config:DataSourceLocation location="${outputLocation}catentrycatentrelnew-output${batchfile}.csv" />
          </_config:LoadItem>
          <_config:LoadItem name="CatalogEntryMasterCategory" businessObjectConfigFile="wc-loader-catalog-entry-catgrp-rel.xml">
            <_config:DataSourceLocation location="${outputLocation}catentrycatgrprel-delete-output${batchfile}.csv" />
          </_config:LoadItem>
          <_config:LoadItem name="CatalogEntryDelete" businessObjectConfigFile="wc-loader-catalog-entry.xml">
            <_config:DataSourceLocation location="${outputLocation}catentry-delete-output${batchfile}.csv" />
          </_config:LoadItem>
        </_config:LoadOrder>

</_config:DataLoadConfiguration>
