<_config:DataLoadConfiguration
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.ibm.com/xmlns/prod/commerce/foundation/config ../xsd/wc-dataload.xsd"
        xmlns:_config="http://www.ibm.com/xmlns/prod/commerce/foundation/config">

        <_config:DataLoadEnvironment configFile="${envPath}wc-dataload-env.xml" />

        <_config:LoadOrder commitCount="1000" batchSize="500" maxError="500000" dataLoadMode="Replace">        

        <_config:LoadItem name="AttributeDictionaryAttribute" businessObjectConfigFile="wc-loader-AD-attribute-and-allowed-values.xml">
          <_config:DataSourceLocation location="${outputLocation}attrdictattr-dataload.csv" />
        </_config:LoadItem>
        		
        <_config:LoadItem name="AttributeDictionaryAttributeAllowedValues" businessObjectConfigFile="wc-loader-attribute-allowed-values.xml">
          <_config:DataSourceLocation location="${outputLocation}attrdictattrval-dataload.csv" />
        </_config:LoadItem>
          
    </_config:LoadOrder>

</_config:DataLoadConfiguration>
