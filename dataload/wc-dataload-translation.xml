<_config:DataLoadConfiguration
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.ibm.com/xmlns/prod/commerce/foundation/config ../xsd/wc-dataload.xsd"
        xmlns:_config="http://www.ibm.com/xmlns/prod/commerce/foundation/config">

        <_config:DataLoadEnvironment configFile="${envPath}wc-dataload-env.xml" />

        <_config:LoadOrder commitCount="1" batchSize="1" maxError="500000" dataLoadMode="Replace">
        <_config:property name="firstTwoLinesAreHeader" value="true" />

        <_config:LoadItem name="AttrDictAttr" businessObjectConfigFile="wc-loader-AD-attribute-and-allowed-values-translation.xml">
          <_config:DataSourceLocation location="${inputLocation}attrdictattr-output.csv" />
        </_config:LoadItem>
        <_config:LoadItem name="AttrDictAttrAllowedValues" businessObjectConfigFile="wc-loader-attribute-allowed-values-translation.xml">
          <_config:DataSourceLocation location="${inputLocation}attrdictattrallowvals-output.csv" />
        </_config:LoadItem>

        <_config:LoadItem name="CatalogEntryPublished" businessObjectConfigFile="wc-loader-catalog-entry-published.xml">
          <_config:DataSourceLocation location="${inputLocation}published-output.csv" />
        </_config:LoadItem>
        <_config:LoadItem name="CatalogEntryName" businessObjectConfigFile="wc-loader-catalog-entry-name.xml">
          <_config:DataSourceLocation location="${inputLocation}name-output.csv" />
        </_config:LoadItem>
        <_config:LoadItem name="CatalogEntryLongDesc" businessObjectConfigFile="wc-loader-catalog-entry-long-desc.xml">
          <_config:DataSourceLocation location="${inputLocation}longdesc-output.csv" />
        </_config:LoadItem>
        <_config:LoadItem name="CatalogEntryKeyword" businessObjectConfigFile="wc-loader-catalog-entry-keyword.xml">
          <_config:DataSourceLocation location="${inputLocation}keyword-output.csv" />
        </_config:LoadItem>

    </_config:LoadOrder>


</_config:DataLoadConfiguration>
