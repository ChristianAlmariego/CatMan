<_config:DataloadBusinessObjectConfiguration
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.ibm.com/xmlns/prod/commerce/foundation/config ./xsd/wc-dataload-businessobject.xsd "
        xmlns:_config="http://www.ibm.com/xmlns/prod/commerce/foundation/config">
  <_config:DataLoader className="com.ibm.commerce.foundation.dataload.BusinessObjectLoader">
    <_config:DataReader className="com.ibm.commerce.foundation.dataload.datareader.CSVReader" useHeaderAsColumnName="true" firstLineIsHeader="true">
        <_config:property value="Identifier" name="keyColumns"/>
     </_config:DataReader>
    <_config:BusinessObjectBuilder className="com.ibm.commerce.foundation.dataload.businessobjectbuilder.TableObjectBuilder" >
		<_config:Table name="CATENTREL" excludeUnListedColumns = "true" deleteKey="Delete" deleteValue="1">

			<_config:Column name="CATENTRY_ID_CHILD" valueFrom="IDResolve" value="CATENTRY_ID_CHILD">
				<_config:IDResolve tableName="CATENTRY" generateNewKey="false" >
					<_config:UniqueIndexColumn name="PARTNUMBER" value="PartNumber" valueFrom="InputData" />
				</_config:IDResolve>
			</_config:Column>

      <_config:Column name="CATENTRY_ID_PARENT" valueFrom="IDResolve" value="CATENTRY_ID_PARENT">
				<_config:IDResolve tableName="CATENTRY" generateNewKey="false" >
					<_config:UniqueIndexColumn name="PARTNUMBER" value="ParentPartNumber" valueFrom="InputData" />
				</_config:IDResolve>
			</_config:Column>

			<_config:Column name="CATRELTYPE_ID" value="Type" />
				</_config:Table>

      <_config:BusinessObjectMediator className="com.ibm.commerce.foundation.dataload.businessobjectmediator.TableObjectMediator" >
      </_config:BusinessObjectMediator>

    </_config:BusinessObjectBuilder>
  </_config:DataLoader>
</_config:DataloadBusinessObjectConfiguration>