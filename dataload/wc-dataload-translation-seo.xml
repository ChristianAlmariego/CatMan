<_config:DataLoadConfiguration
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.ibm.com/xmlns/prod/commerce/foundation/config ../xsd/wc-dataload.xsd"
        xmlns:_config="http://www.ibm.com/xmlns/prod/commerce/foundation/config">

        <_config:DataLoadEnvironment configFile="${envPath}wc-dataload-env.xml" />

        <_config:LoadOrder commitCount="1" batchSize="1" maxError="500" dataLoadMode="Replace">
          <_config:property name="firstTwoLinesAreHeader" value="true" />
                  <_config:property name="loadSEO" value="true"/>
          <_config:LoadItem name="CatalogEntryURLKeyword" businessObjectConfigFile="wc-loader-catalog-entry-urlkeyword.xml">
            <_config:DataSourceLocation location="${inputLocation}urlkeyword-output.csv" />
          </_config:LoadItem>
          <_config:LoadItem name="CatalogEntryPageTitle" businessObjectConfigFile="wc-loader-catalog-entry-page-title.xml">
            <_config:DataSourceLocation location="${inputLocation}pagetitle${langId}-output.csv" />
            <_config:BusinessContext languageId="${langId}" />
          </_config:LoadItem>
          <_config:LoadItem name="CatalogEntryMetaDesc" businessObjectConfigFile="wc-loader-catalog-entry-meta-desc.xml">
            <_config:DataSourceLocation location="${inputLocation}metadesc${langId}-output.csv" />
            <_config:BusinessContext languageId="${langId}" />
          </_config:LoadItem>

        </_config:LoadOrder>

</_config:DataLoadConfiguration>
