<_config:DataLoadConfiguration
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.ibm.com/xmlns/prod/commerce/foundation/config ../xsd/wc-dataload.xsd"
        xmlns:_config="http://www.ibm.com/xmlns/prod/commerce/foundation/config">

        <_config:DataLoadEnvironment configFile="${envPath}wc-dataload-env.xml" />

        <_config:LoadOrder commitCount="1" batchSize="1" maxError="500" dataLoadMode="Replace">
        <_config:property name="firstTwoLinesAreHeader" value="true" />
		<_config:property name="loadSEO" value="true"/>

        <_config:LoadItem name="CatalogGroup" businessObjectConfigFile="wc-loader-catalog-group.xml">
          <_config:DataSourceLocation location="${outputLocation}catgrpmaster-output${batchfile}.csv" />
        </_config:LoadItem>

        <_config:LoadItem name="CatalogGroupName" businessObjectConfigFile="wc-loader-catalog-group-name.xml">
          <_config:DataSourceLocation location="${outputLocation}catgrpname-output${batchfile}.csv" />
        </_config:LoadItem>

        <_config:LoadItem name="CatalogGroupUrlKeyword" businessObjectConfigFile="wc-loader-catalog-group-urlkeyword.xml">
          <_config:DataSourceLocation location="${outputLocation}catgrpurlkeyword-output${batchfile}.csv" />
        </_config:LoadItem>

        <_config:LoadItem name="CatalogGroupMetaDesc" businessObjectConfigFile="wc-loader-catalog-group-meta-desc.xml">
          <_config:DataSourceLocation location="${outputLocation}catgrpmetadesc-output${batchfile}.csv" />
        </_config:LoadItem>

        <_config:LoadItem name="CatalogGroupPageTitle" businessObjectConfigFile="wc-loader-catalog-group-page-title.xml">
          <_config:DataSourceLocation location="${outputLocation}catgrppagetitle-output${batchfile}.csv" />
        </_config:LoadItem>

        <_config:LoadItem name="CatalogGroupShortDesc" businessObjectConfigFile="wc-loader-catalog-group-short-desc.xml">
          <_config:DataSourceLocation location="${outputLocation}catgrpshortdesc-output${batchfile}.csv" />
        </_config:LoadItem>

        <_config:LoadItem name="CatalogGroupPublished" businessObjectConfigFile="wc-loader-catalog-group-published.xml">
          <_config:DataSourceLocation location="${outputLocation}catgrppublished-output${batchfile}.csv" />
        </_config:LoadItem>


    </_config:LoadOrder>

</_config:DataLoadConfiguration>
