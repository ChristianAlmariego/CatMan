<_config:DataloadBusinessObjectConfiguration
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.ibm.com/xmlns/prod/commerce/foundation/config ../../../../xml/config/xsd/wc-dataload-businessobject.xsd"
        xmlns:_config="http://www.ibm.com/xmlns/prod/commerce/foundation/config">

  <_config:DataLoader className="com.ibm.commerce.foundation.dataload.BusinessObjectLoader" >
    <_config:ColumnExclusionList>
      <_config:table name="ATTR" columns="ATTR_ID,IDENTIFIER" />
      <_config:table name="ATTRDESC" columns="ATTR_ID,LANGUAGE_ID,ATTRTYPE_ID,NAME,DESCRIPTION,DESCRIPTION2,FIELD1,GROUPNAME,QTYUNIT_ID,NOTEINFO" forUpdateOnly="true" />
    </_config:ColumnExclusionList>
    <_config:DataReader className="com.ibm.commerce.foundation.dataload.datareader.CSVReader"
          firstLineIsHeader="true" useHeaderAsColumnName="true" >
          <_config:property name="keyColumns" value="Identifier" />
    </_config:DataReader>

    <_config:BusinessObjectBuilder className="com.ibm.commerce.foundation.dataload.businessobjectbuilder.TableObjectBuilder" >
      
      <_config:Table name="ATTR" excludeUnListedColumns="true">
      	<_config:Column name="ATTR_ID" value="ATTR_ID" valueFrom="IDResolve">
 		  <_config:IDResolve tableName="ATTR" generateNewKey="false" >
    	  	<_config:UniqueIndexColumn name="IDENTIFIER" value="Identifier" skipIDResolveIfNull="true" />
    	  	<_config:UniqueIndexColumn name="ATTRDICT_ID" value="10001" valueFrom="Fixed" />
  		  </_config:IDResolve>
		</_config:Column>
		<_config:Column name="IDENTIFIER" value="Identifier" valueFrom="InputData" />
	  </_config:Table>
	  
      <_config:Table name="ATTRDESC" deleteKey="Delete" deleteValue="1" excludeUnListedColumns="true">
      	<_config:Column name="ATTR_ID" value="ATTR_ID" valueFrom="IDResolve">
 		  <_config:IDResolve tableName="ATTR" generateNewKey="false" >
    	  	<_config:UniqueIndexColumn name="ATTR_ID" value="ATTR_ID" valueFrom="IDResolve" />
  		  </_config:IDResolve>
		</_config:Column>
  		<_config:Column name="LANGUAGE_ID" value="langId" valueFrom="BusinessContext" />
  		<_config:Column name="ATTRTYPE_ID" value="Type" valueFrom="InputData" />
    	<_config:Column name="NAME" value="Name" valueFrom="InputData" />
    	<_config:Column name="DESCRIPTION" value="Description" valueFrom="InputData" />
    	<_config:Column name="DESCRIPTION2" value="SecondaryDescription" valueFrom="InputData" />
    	<_config:Column name="FIELD1" value="Field1" valueFrom="InputData" />
    	<_config:Column name="GROUPNAME" value="AssociatedKeyword" valueFrom="InputData" />
    	<_config:Column name="QTYUNIT_ID" value="UnitOfMeasure" valueFrom="InputData" />
    	<_config:Column name="NOTEINFO" value="Footnote" valueFrom="InputData" />
	  </_config:Table>
    
      
    <_config:BusinessObjectMediator className="com.ibm.commerce.foundation.dataload.businessobjectmediator.TableObjectMediator" >
    </_config:BusinessObjectMediator>
 
    </_config:BusinessObjectBuilder>
  </_config:DataLoader>  

</_config:DataloadBusinessObjectConfiguration>
