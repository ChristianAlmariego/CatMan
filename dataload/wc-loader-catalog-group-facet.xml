<_config:DataloadBusinessObjectConfiguration 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.ibm.com/xmlns/prod/commerce/foundation/config ../../../../xml/config/xsd/wc-dataload-businessobject.xsd" 
	xmlns:_config="http://www.ibm.com/xmlns/prod/commerce/foundation/config">

  <_config:DataLoader className="com.ibm.commerce.foundation.dataload.BusinessObjectLoader" >
    <_config:DataReader className="com.ibm.commerce.foundation.dataload.datareader.CSVReader"
          firstLineIsHeader="true" useHeaderAsColumnName="true" >
          <_config:property name="keyColumns" value="CATGROUP_IDENTIFIER,FACET_IDENTIFIER" />
    </_config:DataReader>

    <_config:BusinessObjectBuilder className="com.ibm.commerce.foundation.dataload.businessobjectbuilder.TableObjectBuilder" >
      
      <_config:Table name="ATTR" excludeUnListedColumns="true">
      	<_config:Column name="ATTR_ID" value="ATTR_ID" valueFrom="IDResolve">
 		  <_config:IDResolve tableName="ATTR" generateNewKey="false" >
    	  	<_config:UniqueIndexColumn name="IDENTIFIER" value="FACET_IDENTIFIER" skipIDResolveIfNull="true" />
  		  </_config:IDResolve>
		</_config:Column>
		<_config:Column name="IDENTIFIER" value="FACET_IDENTIFIER" valueFrom="InputData" />
	  </_config:Table>
	  
	  
      
      <_config:Table name="FACETCATGRP" deleteKey="Delete" deleteValue="1" excludeUnListedColumns="true">
      	<_config:Column name="FACETCATGRP_ID" value="FACETCATGRP_ID" valueFrom="IDResolve">
 		  <_config:IDResolve tableName="FACETCATGRP" generateNewKey="true" >
  		  </_config:IDResolve>
		</_config:Column>
 		<_config:Column name="FACET_ID" value="FACET_ID" valueFrom="IDResolve" >
 		  <_config:IDResolve tableName="FACET" generateNewKey="false" >
    	  	<_config:UniqueIndexColumn name="ATTR_ID" value="ATTR_ID" valueFrom="IDResolve" />
  		  </_config:IDResolve>
    	</_config:Column>
  		<_config:Column name="SEQUENCE" value="SEQUENCE" valueFrom="InputData" />
    	<_config:Column name="CATGROUP_ID" value="CATGROUP_ID" valueFrom="IDResolve">
    	  <_config:IDResolve tableName="CATGROUP" generateNewKey="false" >
    	  	<_config:UniqueIndexColumn name="IDENTIFIER" value="CATGROUP_IDENTIFIER" />
  		  </_config:IDResolve>
    	</_config:Column>
    	<_config:Column name="STOREENT_ID" value="STOREENT_ID" valueFrom="InputData" />
    	<_config:Column name="DISPLAYABLE" value="DISPLAYABLE" valueFrom="InputData" />
	  </_config:Table>
    
      
    <_config:BusinessObjectMediator className="com.ibm.commerce.foundation.dataload.businessobjectmediator.TableObjectMediator" >
    </_config:BusinessObjectMediator>
 
    </_config:BusinessObjectBuilder>
  </_config:DataLoader>  

</_config:DataloadBusinessObjectConfiguration>
 