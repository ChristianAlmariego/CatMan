<_config:DataLoadConfiguration
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.ibm.com/xmlns/prod/commerce/foundation/config ../xsd/wc-dataload.xsd"
        xmlns:_config="http://www.ibm.com/xmlns/prod/commerce/foundation/config">

        <_config:DataLoadEnvironment configFile="${envPath}wc-dataload-env.xml" />

        <_config:LoadOrder commitCount="1" batchSize="500" maxError="500" dataLoadMode="Replace">
        <_config:property name="firstTwoLinesAreHeader" value="true" />
        
        <_config:LoadItem name="CatalogEntryAttributes" businessObjectConfigFile="wc-loader-catentry-attributes.xml">
          <_config:DataSourceLocation location="${outputLocation}catentry-attributes-${processId}.csv" />
        </_config:LoadItem>
        
    </_config:LoadOrder>

</_config:DataLoadConfiguration>
