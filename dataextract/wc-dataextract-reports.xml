<_config:DataLoadConfiguration
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.ibm.com/xmlns/prod/commerce/foundation/config ../xsd/wc-dataload.xsd"
        xmlns:_config="http://www.ibm.com/xmlns/prod/commerce/foundation/config">

        <_config:DataLoadEnvironment configFile="${envPath}wc-dataextract-env.xml" />

        <_config:LoadOrder commitCount="0" batchSize="0">
        <_config:property name="firstLineIsHeader" value="true" />
       
        <!-- Extract Product Usage  -->
        <_config:LoadItem name="ProductAttributeRelationship" businessObjectConfigFile="wc-extract-catentryattr-attrdeletion-runnable.xml">
          <_config:DataOutputLocation location="${outputLocation}usage.csv" />
        </_config:LoadItem>

        <!-- Extract Value Usage  -->
        <_config:LoadItem name="AttrValueUsageRelationship" businessObjectConfigFile="wc-extract-catentryattr-attrvalusage-runnable.xml">
          <_config:DataOutputLocation location="${outputLocation}usage.csv" />
        </_config:LoadItem>
    </_config:LoadOrder>

</_config:DataLoadConfiguration>
