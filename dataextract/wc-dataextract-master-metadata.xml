<_config:DataLoadConfiguration
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.ibm.com/xmlns/prod/commerce/foundation/config ../xsd/wc-dataload.xsd"
        xmlns:_config="http://www.ibm.com/xmlns/prod/commerce/foundation/config">

        <_config:DataLoadEnvironment configFile="${envPath}wc-dataextract-env.xml" />

        <_config:LoadOrder commitCount="0" batchSize="0">
        <_config:property name="firstTwoLinesAreHeader" value="true" />

        <!-- Extract all Catalog Entries  -->
        <_config:LoadItem name="CatalogEntrySEOPageTitle" businessObjectConfigFile="wc-extract-catalogentrypagetitle-runnable.xml">
          <_config:DataOutputLocation location="${outputLocation}pagetitle${langId}-output-raw.csv" />
          <_config:BusinessContext languageId="${langId}" />
        </_config:LoadItem>
        <_config:LoadItem name="CatalogEntrySEOPageTitleItemLevel" businessObjectConfigFile="wc-extract-catalogentrypagetitle-item-runnable.xml">
          <_config:DataOutputLocation location="${outputLocation}pagetitle${langId}-output-raw.csv" />
          <_config:BusinessContext languageId="${langId}" />
        </_config:LoadItem>
        <_config:LoadItem name="CatalogEntrySEOMetaDesc" businessObjectConfigFile="wc-extract-catalogentrymetadesc-runnable.xml">
          <_config:DataOutputLocation location="${outputLocation}metadesc${langId}-output-raw.csv" />
          <_config:BusinessContext languageId="${langId}" />
        </_config:LoadItem>
        <_config:LoadItem name="CatalogEntrySEOMetaDescItemLevel" businessObjectConfigFile="wc-extract-catalogentrymetadesc-item-runnable.xml">
          <_config:DataOutputLocation location="${outputLocation}metadesc${langId}-output-raw.csv" />
          <_config:BusinessContext languageId="${langId}" />
        </_config:LoadItem>

    </_config:LoadOrder>

</_config:DataLoadConfiguration>
