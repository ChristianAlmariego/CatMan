								(SELECT G.IDENTIFIER FROM CATGROUP G WHERE G.CATGROUP_ID IN
									(SELECT FIRST_VALUE(CGER.CATGROUP_ID) OVER (ORDER BY LASTUPDATE DESC ROWS UNBOUNDED PRECEDING) FROM CATGPENREL CGER WHERE CGER.CATALOG_ID = 10001 
										AND CGER.CATENTRY_ID = CATENTRY.CATENTRY_ID)) PARENTGROUPIDENTIFIER,

 CGER.PARENTGROUPIDENTIFIER,
				JOIN
								(SELECT CATGPENREL.CATENTRY_ID, FIRST_VALUE(CATGPENREL.CATGROUP_ID) PARENTGROUPIDENTIFIER OVER (ORDER BY CATGPENREL.LASTUPDATE ASC ROWS UNBOUNDED PRECEDING)
								 FROM CATGPENREL 
								 WHERE CATGPENREL.CATALOG_ID = 10001) CGER ON (CGER.CATENTRY_ID = CATENTRY.CATENTRY_ID)
-->
<_config:DataloadBusinessObjectConfiguration
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.ibm.com/xmlns/prod/commerce/foundation/config ../../../WC/xml/config/xsd/wc-dataload-businessobject.xsd"
        xmlns:_config="http://www.ibm.com/xmlns/prod/commerce/foundation/config">

  <_config:DataLoader className="com.ibm.commerce.foundation.dataload.BusinessObjectLoader">
    <_config:DataReader className="com.ibm.commerce.foundation.dataload.datareader.UniqueIdReader">
            <_config:Query>
              <_config:SQL>
                    <![CDATA[
				SELECT
								CATENTRY.CATENTRY_ID
				FROM
								CATENTRY
				WHERE
								CATENTRY.MARKFORDELETE = 0
									AND CATENTRY.MEMBER_ID = (SELECT MEMBER_ID FROM STOREENT WHERE STOREENT_ID = ?)
				ORDER BY
								CATENTRY.PARTNUMBER
                    ]]>
              </_config:SQL>
              <_config:Param name="storeId" valueFrom="BusinessContext" />
            </_config:Query>

    </_config:DataReader>
        <_config:BusinessObjectBuilder className="com.ibm.commerce.foundation.dataload.businessobjectbuilder.PassThroughBusinessObjectBuilder">
                <_config:BusinessObjectMediator className="com.ibm.commerce.foundation.dataload.businessobjectmediator.AssociatedObjectMediator">

                        <_config:Query>
                                <_config:SQL>
                                        <![CDATA[
				SELECT 
								CATENTRY.PARTNUMBER,
								DECODE(CATENTRY.CATENTTYPE_ID,  'ProductBean', 'Product',
																'ItemBean', 'Item',
																'BundleBean', 'Bundle',
																'PackageBean', 'Package',
																'DynamicKitBean', 'Dynamic',
																'CatalogEntryBean') TYPE,
								CATENTPARENT.PARTNUMBER PARENTPARTNUMBER, CATGROUP.IDENTIFIER PARENTGROUPIDENTIFIER,
								STOREENT.IDENTIFIER STOREIDENTIFIER, CATENTREL.SEQUENCE,
								CATENTRY.MFPARTNUMBER, CATENTRY.MFNAME, CATENTRY.ONSPECIAL, CATENTRY.ONAUCTION, CATENTRY.BUYABLE,
								CATENTRY.URL, CATENTRY.FIELD1, CATENTRY.FIELD2, CATENTRY.FIELD3, CATENTRY.FIELD4, CATENTRY.FIELD5,
								NVL(CATENTSUBS.SUBSCPTYPE_ID, 'NONE') SUBSCPTYPE_ID, NVL(CATENTSUBS.DISALLOW_REC_ORDER, 1) DISALLOW_REC_ORDER
				FROM
								CATENTRY
				JOIN
								STOREENT ON (STOREENT.MEMBER_ID = CATENTRY.MEMBER_ID)
				JOIN
								(SELECT DISTINCT CATGPENREL.CATENTRY_ID, FIRST_VALUE(CATGPENREL.CATGROUP_ID) OVER
									(PARTITION BY CATGPENREL.CATENTRY_ID ORDER BY CATGPENREL.LASTUPDATE ASC ROWS UNBOUNDED PRECEDING) CATGROUP_ID
								 FROM CATGPENREL 
								 WHERE CATGPENREL.CATALOG_ID = 10001) CGER ON (CGER.CATENTRY_ID = CATENTRY.CATENTRY_ID)
				JOIN
								CATGROUP ON (CATGROUP.CATGROUP_ID = CGER.CATGROUP_ID)
				LEFT OUTER JOIN 
								CATENTSUBS ON (CATENTSUBS.CATENTRY_ID = CATENTRY.CATENTRY_ID)
				LEFT OUTER JOIN 
								CATENTREL ON (CATENTREL.CATENTRY_ID_CHILD = CATENTRY.CATENTRY_ID AND CATRELTYPE_ID = 'PRODUCT_ITEM')
				LEFT OUTER JOIN 
								CATENTRY CATENTPARENT ON (CATENTPARENT.CATENTRY_ID = CATENTREL.CATENTRY_ID_PARENT)
				WHERE
								CATENTRY.CATENTRY_ID = ?
                                        ]]>
                                        </_config:SQL>
                <_config:Param name="CATENTRY_ID" />
                                <_config:ColumnMapping name="PARTNUMBER" value="PartNumber" />
                                <_config:ColumnMapping name="TYPE" value="Type" />
                                <_config:ColumnMapping name="PARENTPARTNUMBER" value="ParentPartNumber" />
                                <_config:ColumnMapping name="PARENTGROUPIDENTIFIER" value="ParentGroupIdentifier" />
                                <_config:ColumnMapping name="STOREIDENTIFIER" value="ParentStoreIdentifier" />
                                <_config:ColumnMapping name="SEQUENCE" value="Sequence" />
                                <_config:ColumnMapping name="MFPARTNUMBER" value="ManufacturerPartNumber" />
                                <_config:ColumnMapping name="MFNAME" value="Manufacturer" />
                                <_config:ColumnMapping name="ONSPECIAL" value="OnSpecial" />
                                <_config:ColumnMapping name="ONAUCTION" value="OnAuction" />
                                <_config:ColumnMapping name="BUYABLE" value="Buyable" />
                                <_config:ColumnMapping name="URL" value="URL" />
                                <_config:ColumnMapping name="FIELD1" value="Field1" />
                                <_config:ColumnMapping name="FIELD2" value="Field2" />
                                <_config:ColumnMapping name="FIELD3" value="Field3" />
                                <_config:ColumnMapping name="FIELD4" value="Field4" />
                                <_config:ColumnMapping name="FIELD5" value="Field5" />
                                <_config:ColumnMapping name="SUBSCPTYPE_ID" value="SUBSCPTYPE_ID" />
                                <_config:ColumnMapping name="DISALLOW_REC_ORDER" value="DISALLOW_REC_ORDER" />
            </_config:Query>

                        <_config:DataWriter className="com.ibm.commerce.foundation.dataload.datawriter.CSVWriter">
                                <_config:Data>
                                        <_config:column name="PartNumber" number="1"/>
                                        <_config:column name="Type" number="2"/>
                                        <_config:column name="ParentPartNumber" number="3"/>
                                        <_config:column name="ParentGroupIdentifier" number="4"/>
                                        <_config:column name="ParentStoreIdentifier" number="5"/>
                                        <_config:column name="Sequence" number="6"/>
                                        <_config:column name="ManufacturerPartNumber" number="7"/>
                                        <_config:column name="Manufacturer" number="8"/>
                                        <_config:column name="OnSpecial" number="9"/>
                                        <_config:column name="OnAuction" number="10"/>
                                        <_config:column name="Buyable" number="11"/>
                                        <_config:column name="URL" number="12"/>
                                        <_config:column name="Field1" number="13"/>
                                        <_config:column name="Field2" number="14"/>
                                        <_config:column name="Field3" number="15"/>
                                        <_config:column name="Field4" number="16"/>
                                        <_config:column name="Field5" number="17"/>
                                        <_config:column name="SUBSCPTYPE_ID" number="18"/>
                                        <_config:column name="DISALLOW_REC_ORDER" number="19"/>
                                        <_config:column name="Delete" number="20" value="" />
                                </_config:Data>
                        </_config:DataWriter>

          </_config:BusinessObjectMediator>
    </_config:BusinessObjectBuilder>
  </_config:DataLoader>

</_config:DataloadBusinessObjectConfiguration>
